{
  "presets": [
    "es2015",
    "stage-0"
  ],
  "env": {
    "default": {
      "plugins": [
        "strova-assert"
      ]
    },
    "info": {
      "plugins": [
        [
          "strova-assert",
          {
            "action": "log_info"
          }
        ]
      ]
    },
    "throw": {
      "plugins": [
        [
          "strova-assert",
          {
            "action": "throw"
          }
        ]
      ]
    }
  }
}
